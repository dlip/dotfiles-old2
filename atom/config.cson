  "imdone-atom": {}
  "markdown-preview":
    useGitHubStyle: true
  "markdown-writer": {}
  minimap:
    plugins:
      "highlight-selected": true
  navigate: {}
  nvatom:
    directory: "C:\\Users\\danel\\Dropbox\\Notes"
  "php-cs-fixer":
    executablePath: "/home/dane/.composer/vendor/bin/php-cs-fixer"
    executeOnSave: true
    showInfoNotifications: true
  "terminal-plus":
    core: {}
    style:
      animationSpeed: 0
  "vim-mode":
