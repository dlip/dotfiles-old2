      "emmet"
      "color-picker"
      "minimap"
      "minimap-highlight-selected"
      "linter-php"
    ]
    themes: [
      "atom-dark-ui"
      "base16-tomorrow-dark-theme"
    ]
  editor:
    fontSize: 16
    invisibles: {}
    showInvisibles: true
    tabType: "soft"
  "exception-reporting":
